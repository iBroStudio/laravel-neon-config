{
    "name": "ibrostudio/laravel-neon-config",
    "description": "Add Neon config values to Laravel config'",
    "keywords": [
        "iBroStudio",
        "laravel",
        "laravel-neon-config"
    ],
    "homepage": "https://github.com/ibrostudio/laravel-neon-config",
    "license": "MIT",
    "authors": [
        {
            "name": "iBroStudio",
            "email": "hello@ibro.studio",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.0",
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "IBroStudio\\NeonConfig\\": "src/",
            "IBroStudio\\NeonConfig\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IBroStudio\\NeonConfig\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "IBroStudio\\NeonConfig\\NeonConfigServiceProvider"
            ],
            "aliases": {
                "NeonConfig": "IBroStudio\\NeonConfig\\Facades\\NeonConfig"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
